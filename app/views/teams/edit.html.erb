<h1>Edit Team</h1>

<%= simple_form_for @team, html:{class: "form-horizontal"} do |f|%>
  <div class="form-group row">
  <%=f.input :team_name, label: "Team Name" %>
  </div>
  <div class = "form-group row">
  <%=f.input :team_logo, as: :file %>
  </div>
  <div class = "form-group row">
  <%=f.input :recognition, label: "Recognition" %>
  </div>
  <div class = "form-group row">
  <%=f.submit "Submit"%>
<% end %>
